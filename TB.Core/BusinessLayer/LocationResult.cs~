using System;

namespace TaskBuddi
{
    public class LocationResult
    {
        public Geometry geometry;
        public string name, icon, id, place_id, reference, scope, vicinity;
        public string[] types;

        public LocationResult()
        {
            
        }
    }

    class Geometry
    {
        public Location Location;

        public Geometry()
        {            
        }
    }

    class Location
    {
        public decimal lat;
        public decimal lng;

        public Location()
        {
        }

    }
}

